<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.composetestproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeTestProject">
        <activity
            android:name=".ExampleActivity"
            android:exported="false"
            android:label="@string/title_activity_example"
            android:theme="@style/Theme.ComposeTestProject" />
        <activity
            android:name=".activity.HomeActivity"
            android:exported="true"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.ComposeTestProject">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SettingActivity"
            android:exported="false"
            android:label="@string/title_activity_setting"
            android:theme="@style/Theme.ComposeTestProject" />
        <activity
            android:name=".activity.RoomActivity"
            android:exported="false"
            android:label="@string/title_activity_room"
            android:theme="@style/Theme.ComposeTestProject" />
        <activity
            android:name=".activity.PagingActivity"
            android:exported="false"
            android:label="@string/title_activity_paging"
            android:theme="@style/Theme.ComposeTestProject" />
        <activity
            android:name=".activity.LazyColumnActivity"
            android:exported="false"
            android:label="@string/title_activity_lazy_column"
            android:theme="@style/Theme.ComposeTestProject" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeTestProject"></activity>
    </application>

</manifest>